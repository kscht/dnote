{
  "name": "deadlynote-frontend",
  "description": "Deadly note app",
  "version": "0.1.0",
  "bun": {
    "runtime": "bun"
  },
  "scripts": {
    "serve": "bun vite --port 4200 --host 0.0.0.0",
    "build": "bun vite build",
    "lint": "bun vite lint"
  },
  "main": "public/index.html",
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.6.0",
    "crypto-js": "^4.2.0",
    "vite": "^5.2.10",
    "vue": "^3.4.24",
    "vue-router": "^4.3.2",
    "vuetify": "^3.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.3",
    "vue-cli-plugin-vuetify": "^0.2.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
